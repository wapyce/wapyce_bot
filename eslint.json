{
    "extends": "eslint:recommended",
    "env": {"node": true},
    "parserOptions": {"ecmaVersion": 8},
    "rules": {
        "no-console": "off",
        "no-alert": "error",    
        "no-undef": "off",
        "no-eval": "error",
        "brace-style": "error",
        "comma-dangle": "error",
        "comma-spacing": "error",
        "comma-style": "error",
        "indent": ["error", 4],
        "max-len": "error",
        "no-tabs": "error",
        "no-trailing-spaces": "error",
        "no-whitespace-before-property": "error"
    }
}
